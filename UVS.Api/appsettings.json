{
  "ConnectionStrings": {
    "UVS": "",
    "Cache": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "UVS.Api"
    }
  },
  "System":{
      "Outbox": {
        "IntervalInSeconds" : 15,
        "BatchSize": 20
      },
    "Inbox": {
      "IntervalInSeconds" : 15,
      "BatchSize": 20
    }
  },
  "Authentication":{
    "Audience": "",
    "TokenValidationParameters": {
      "ValidIssuers": []
    },
    "MetadataAddress": "",
    "RequireHttpsMetadata": false
  },
  "Users":{
    "KeyCloak": {
      
    },
    "Outbox": {
      "IntervalInSeconds": 15,
      "BatchSize": 20
    },
    "Inbox": {
      "IntervalInSeconds" : 15,
      "BatchSize": 20
    }
  } 
}
